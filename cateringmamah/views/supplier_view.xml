<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- cateringmamah.supplier tree view -->
    <record id="cateringmamah_supplier_view_tree" model="ir.ui.view">
        <field name="name">Daftar Supplier</field>
        <field name="model">cateringmamah.supplier</field>
        <field name="arch" type="xml">
            <tree>
                <!-- Add your fields here -->
                <field name="name" />
                <field name="alamat" />
                <field name="no_telp" />
                <field name="makanan_id" />
                <field name="makanan_id" />
            </tree>
        </field>
    </record>

    <!-- cateringmamah.supplier form view -->
    <record id="cateringmamah_supplier_view_form" model="ir.ui.view">
        <field name="name">Form Supplier</field>
        <field name="model">cateringmamah.supplier</field>
        <field name="arch" type="xml">
            <form string="Supplier">
                <sheet>
                    <group>
                        <!-- Add your fields here -->
                        <field name="name" />
                        <field name="alamat" />
                        <field name="no_telp" />
                    </group>
                    <group name="makanan" string="Makanan">
                        <field name="makanan_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- cateringmamah.supplier action window -->
    <record id="cateringmamah_supplier_action" model="ir.actions.act_window">
        <field name="name">Supplier</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">cateringmamah.supplier</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- This Menu Item must have a parent and an action -->
    <menuitem id="cateringmamah_supplier_menu_act" 
        name="Supplier" 
        parent="cateringmamah_menu_root" 
        action="cateringmamah_supplier_action" 
        sequence="40"/>

</odoo>